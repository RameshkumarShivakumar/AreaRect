<India>:
    MDFloatLayout:
        MDBottomNavigation:
            MDBottomNavigationItem:
                name:"Rectangle"
                text:"Rectangle"
                icon:'rectangle'
                MDTextField:
                    id:a1
                    hint_text:"length"
                    mode:"rectangle"
                    size_hint:0.70,None
                    pos_hint:{"center_x":0.50,"center_y":0.70}
                MDTextField:
                    id:a2
                    hint_text:"Breadth"
                    mode:"rectangle"
                    size_hint:0.70,None
                    pos_hint:{"center_x":0.50,"center_y":0.50}
                MDRaisedButton:
                    text:"Result"
                    pos_hint:{"center_x":0.50,"center_y":0.30}
                    on_release:root.resulting(self,a1.text,a2.text)
                MDLabel:
                    id:stylish1
                    text:" 0"
                    pos_hint:{"center_x":0.91,"center_y":0.10}
                    font_size:50
            MDBottomNavigationItem:
                name:"Rectangle2"
                text:"Triangle"
                icon:'triangle'
                MDTextField:
                    id:aa1
                    hint_text:"a"
                    mode:"rectangle"
                    size_hint:0.70,None
                    pos_hint:{"center_x":0.50,"center_y":0.90}
                MDTextField:
                    id:bb1
                    hint_text:"b"
                    mode:"rectangle"
                    size_hint:0.70,None
                    pos_hint:{"center_x":0.50,"center_y":0.70}
                MDTextField:
                    id:cc1
                    hint_text:"c"
                    mode:"rectangle"
                    size_hint:0.70,None
                    pos_hint:{"center_x":0.50,"center_y":0.50}
                MDRaisedButton:
                    text:"Result"
                    pos_hint:{"center_x":0.50,"center_y":0.30}
                    on_release:root.rating(self,aa1.text,bb1.text,cc1.text)
                MDLabel:
                    id:stylish2
                    text:"0"
                    pos_hint:{"center_x":0.91,"center_y":0.10}
                    font_size:50

            
